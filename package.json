{
  "name": "smartapes-backend-ts",
  "version": "0.2.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test:unit": "jest --maxWorkers=50% --colors --verbose --detectOpenHandles --testPathPattern=.*\\.spec\\.ts --testPathIgnorePatterns=.*.integration.spec.ts",
    "test:integration": "jest --maxWorkers=50% --colors --verbose --detectOpenHandles --testPathPattern=.*integration.*\\.spec\\.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit:dev": "jest --testPathPattern=.*\\.spec\\.ts --testPathIgnorePatterns=.*.integration.spec.ts --watchAll",
    "test:integration:dev": "jest --testPathPattern=.*integration.*\\.spec\\.ts --watchAll --runInBand",
    "test:changed": "jest --runInBand --onlyChanged",
    "start": "npm run build && node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "rimraf dist && tsc",
    "db:create:dev": "cross-env NODE_ENV=development node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test node scripts/db/create",
    "db:delete:dev": "cross-env NODE_ENV=development node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test node scripts/db/delete",
    "db:flush:dev": "cross-env NODE_ENV=development npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "cross-env NODE_ENV=test npm run db:delete:test && npm run db:create:test",
    "migrate:dev": "npm run build && cross-env NODE_ENV=development sequelize db:migrate",
    "migrate:test": "npm run build && cross-env NODE_ENV=test sequelize db:migrate",
    "migrate:undo:dev": "npm run build && cross-env NODE_ENV=development sequelize db:migrate:undo",
    "migrate:undo:test": "npm run build && cross-env NODE_ENV=test sequelize db:migrate:undo",
    "seed:dev:all": "npm run build && cross-env NODE_ENV=development sequelize db:seed:all",
    "seed:undo:dev:all": "npm run build && cross-env NODE_ENV=development sequelize db:seed:undo:all",
    "seed:test:all": "npm run build && cross-env NODE_ENV=test sequelize db:seed:all",
    "seed:undo:test:all": "npm run build && cross-env NODE_ENV=test sequelize db:seed:undo:all",
    "setup:dev": "npm install && npm run db:flush:dev && npm run migrate:dev",
    "setup:test": "npm install && npm run db:flush:test && npm run migrate:test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.395.0",
    "@hokify/agenda": "^6.3.0",
    "awesome-phonenumber": "^5.9.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.5",
    "nodemailer-mailgun-transport": "^2.1.5",
    "pino": "^8.15.0",
    "sequelize": "^6.31.0",
    "sharp": "^0.32.5",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.82",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.29",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "@types/nodemailer": "^6.4.9",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.0",
    "jest": "^29.6.4",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "validate-branch-name": "^1.3.0"
  }
}
